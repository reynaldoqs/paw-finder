{
	"tailwindCSS.experimental.classRegex": [
		["([\"'`][^\"'`]*.*?[\"'`])", "[\"'`]([^\"'`]*).*?[\"'`]"]
	],
	"cSpell.words": [
		"cmdk",
		"hugeicons",
		"lineicons",
		"lineiconshq",
		"shadcn",
		"stepperize",
		"SUPABASE",
		"turbopack"
	],
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.biome": "explicit"
	}
}
